<plugin name='standalone' version='1.2.3' grailsVersion='1.3 &gt; *'>
  <author>Burt Beckwith</author>
  <authorEmail>burt@burtbeckwith.com</authorEmail>
  <title>Standalone App Runner</title>
  <description>Runs a Grails application as a JAR file with an embedded Tomcat or Jetty server</description>
  <documentation>http://grails-plugins.github.com/grails-standalone/</documentation>
  <type>StandaloneGrailsPlugin</type>
  <resources />
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='org.eclipse.jetty.aggregate' name='jetty-all' version='7.6.0.v20120127' />
    </compile>
  </dependencies>
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>